  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "dotenv": "^16.4.1"
  },
  "dependencies": {
    "@intercoin/liquidity": "^1.0.0",
    "@intercoin/releasemanager": "^2.0.0",
    "@openzeppelin/contracts": "^4.7.3",
    "@openzeppelin/contracts-upgradeable": "^4.7.3",
    "@openzeppelin/test-helpers": "^0.5.16",
